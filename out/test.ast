Func(
    "main",
    [
        LetMut(
            "n",
            Value(
                Complex(
                    2,
                    0,
                ),
            ),
        ),
        Let(
            "max",
            Value(
                Complex(
                    100,
                    0,
                ),
            ),
        ),
        While(
            NotEquals(
                (
                    Id(
                        "n",
                    ),
                    Id(
                        "max",
                    ),
                ),
            ),
            [
                LetMut(
                    "k",
                    Value(
                        Complex(
                            2,
                            0,
                        ),
                    ),
                ),
                LetMut(
                    "found",
                    Value(
                        Complex(
                            0,
                            0,
                        ),
                    ),
                ),
                While(
                    NotEquals(
                        (
                            Id(
                                "k",
                            ),
                            Id(
                                "n",
                            ),
                        ),
                    ),
                    [
                        If(
                            Equals(
                                (
                                    Remainder(
                                        (
                                            Id(
                                                "n",
                                            ),
                                            Id(
                                                "k",
                                            ),
                                        ),
                                    ),
                                    Value(
                                        Complex(
                                            0,
                                            0,
                                        ),
                                    ),
                                ),
                            ),
                            [
                                Assign(
                                    "found",
                                    Value(
                                        Complex(
                                            1,
                                            0,
                                        ),
                                    ),
                                ),
                                Break,
                            ],
                        ),
                        AddAssign(
                            "k",
                            Value(
                                Complex(
                                    1,
                                    0,
                                ),
                            ),
                        ),
                    ],
                ),
                Print(
                    Id(
                        "k",
                    ),
                ),
                IfElse(
                    Id(
                        "found",
                    ),
                    [
                        PrintLitLn(
                            " is prime",
                        ),
                    ],
                    [
                        PrintLitLn(
                            " is not prime",
                        ),
                    ],
                ),
                AddAssign(
                    "n",
                    Value(
                        Complex(
                            1,
                            0,
                        ),
                    ),
                ),
            ],
        ),
    ],
)